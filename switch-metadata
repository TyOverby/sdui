    "sexplib0" {>= "v0.12"}
    "sexplib0" {with-test & >= "v0.15"}
    "stdlib-shims"
    "ocamlfind" {with-test}
    "re" {with-test & >= "1.9.0"}
    "cinaps" {with-test & >= "v0.12.1"}
    "odoc" {with-doc}
  ]
  conflicts: [
    "ocaml-migrate-parsetree" {< "2.0.0"}
    "ocaml-base-compiler" {= "5.1.0~alpha1"}
    "ocaml-variants" {= "5.1.0~alpha1+options"}
    "base-effects"
  ]
  build: [
    ["dune" "subst"] {dev}
    [
      "dune"
      "build"
      "-p"
      name
      "-j"
      jobs
      "@install"
      "@runtest" {with-test}
      "@doc" {with-doc}
    ]
